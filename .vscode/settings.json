{
    "files.associations": {
        "*.txt": "lua",
    },

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.meta": true
    },
    "git.ignoreLimitWarning": true,
    "lua.targetVersion": "5.3"
}